{
  "name": "lazy-thunk",
  "version": "0.1.0",
  "description": "A set of data structures in TypeScript which utilizes lazy evaluation",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./LinkedList": {
      "types": "./dist/LinkedList/index.d.ts",
      "import": "./dist/LinkedList/index.mjs",
      "require": "./dist/LinkedList/index.cjs"
    }
  },
  "scripts": {
    "test": "jest",
    "build": "rollup --config --configPlugin typescript",
    "build:tsc": "tsc",
    "lint": "tsc --noEmit && eslint .",
    "prepare": "husky install"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/J3m3/lazy-thunk.git"
  },
  "homepage": "https://github.com/J3m3/lazy-thunk/blob/main/README.md",
  "bugs": {
    "url": "https://github.com/J3m3/lazy-thunk/issues"
  },
  "engines": {
    "node": ">=16.15.1"
  },
  "keywords": [
    "TypeScript",
    "Lazy Evaluation",
    "Thunk"
  ],
  "author": "J3m3",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "rollup": "^4.9.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
